<%= search_form_for @q, remote: false do |f| %>
  <%= f.search_field :name_or_menu_timings_open_from_cont, placeholder:"Search", class: "right-btn" %>
  <%= f.label "Weekdays"%>
  <%= f.select :menu_timings_weekdays_eq, MenuTiming.weekdays.to_a.map { |w| [w[0].humanize, w[1]] },
                         {:include_blank => true} %>
  <%= f.submit%>
<% end %>
