<%= search_form_for @q do |f| %>
  <%= f.search_field :customer_first_name_or_customer_last_name_or_order_items_orderable_of_FoodItem_type_name_or_order_items_orderable_of_GroupItem_type_name_or_customer_city_or_customer_state_or_customer_street_address_or_total_amount_or_customer_full_name_cont, placeholder:"Search....", class: "right-btn"%>
  <%= f.label "Status:"%>
  <%= f.select :status_eq, Order.statuses.to_a.map { |w| [w[0].humanize, w[1]] },
                         {:include_blank => true} %>
  <%= f.submit%>
<% end %>
